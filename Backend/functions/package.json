{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --import=data --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha --require ts-node/register test/reepayUtils.test.ts",
    "kill": "lsof -t -i:5001 -i:5002 -i:8080 -i:9000 -i:9099 -i:9199 -i:9090 -i:8085 | xargs kill -9",
    "kill2": "taskkill /f /im java.exe"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^2.19.0",
    "@sendgrid/mail": "^7.6.2",
    "@slack/bolt": "^3.11.0",
    "@slack/socket-mode": "^1.3.0",
    "@slack/web-api": "^6.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "firebase": "^9.8.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.21.0",
    "firebase-tools": "^10.9.2",
    "node-fetch": "^2.6.7",
    "tsscmp": "^1.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.3.3",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "optionator": "^0.9.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4"
  },
  "private": true
}
